  -11847286243/1099511627776, 1084116104452462070609082665064238307/170141183460469231731687303715884105728, 0
};

(* Filter eigenpoints with modulus approximately 1 *)
filteredEigenpoints = Select[eigenpoints, 0.98 <= Norm[#] <= 1.02 &];

(* Define the homotopy function *)
homotopy[z_, t_] := (1 - t) z + 
  t (z + Sum[coefficientsJungreis[[k]] / z^(k - 1), {k, 1, Length[coefficientsJungreis]}]);

(* Compute and print snapshots at t = 0, 0.25, 0.5, 0.75, 1 *)
Do[
  Module[{mappedPoints, plot},
    
    (* Apply homotopy at t *)
    mappedPoints = ReIm[homotopy[#1 + I #2, t]] & @@@ filteredEigenpoints;
    
    (* Generate plot *)
    plot = ListPlot[mappedPoints, AspectRatio->Automatic,PlotStyle -> {PointSize[Medium], Red}, 
      Axes -> True, Frame -> True, 
      PlotLabel -> StringJoin["Homotopy at t = ", ToString[t]]];
    
    (* Print each plot separately *)
    Print[plot];
  ],
  {t, {0, 0.25, 0.5, 0.75, 1}}
];
